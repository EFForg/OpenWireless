service netperf
{
	disable         = no
	socket_type     = stream
	wait            = no
	user            = nobody
	port		= 12865
	type		= UNLISTED
	server          = /usr/bin/netserver
	server_args	= -N
	flags		= IPv6
	log_on_success  += HOST USERID
	log_on_failure  += HOST USERID
	instances	= 16
}

